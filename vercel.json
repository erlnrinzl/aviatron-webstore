{
  "public": "src",
  "routes": [
    { "src": "/", "dest": "pages/index.html" },
    { "src": "../styles/(.*)", "dest": "styles/$1" },
    { "src": "/pages/(.*)", "dest": "pages/$1" },
    { "src": "../js/(.*)", "dest": "js/$1" },
    { "src": "../assets/(.*)", "dest": "assets/$1" },
    { "src": "../data/(.*)", "dest": "data/$1" }
  ]
}
